platform :ios, '9.0'
use_frameworks!

def pods
	pod 'SwiftLint'
	pod 'Alamofire'
	pod 'LayoutKit'
	pod 'FPSCounter'
	pod 'KeychainAccess'
	pod 'IQKeyboardManagerSwift'
	pod 'Kingfisher'
	pod 'NextGrowingTextView'
  end

target 'FSPO' do
	pods
end
target 'FSPOUITests' do
	pods
end